{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": ["airbnb-base", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["import"],
  "rules": {
    "no-unused-vars": ["warn"],
    "no-underscore-dangle": ["warn"],

    "import/no-extraneous-dependencies": ["warn"]
  }
}
